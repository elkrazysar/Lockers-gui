local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Lockers Gui", "DarkTheme")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Lockers and other stuffs")
Section:NewButton("VascalLock", "VascalLock", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Gwaporoblox/VascalLock/main/README.md"))()
end)
Section:NewButton("G lock", "G lock", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/elkrazysar/tester6/main/tester6" ))()
end)
Section:NewButton("Awaken v4", "Awaken v4", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Nahhhbruhhh12/Awakenv4done/main/Awakenv4"))()
end)
Section:NewButton("Anti lock", "Anti lock", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/wenny69420/KirbswareScripts/main/MobileV3'))()
end)
Section:NewButton("Infiniteyield", "Infiniteyield", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)
Section:NewButton("Marco script", "Credits for IChrizz", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/aaiVnnQq"))()
end)
Section:NewButton("Random lock", "random lock", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/XNBfsZJq"))()
end)
local Tab = Window:NewTab("Credits tab")
local Section = Tab:NewSection("Credits")
Section:NewLabel("Credits for IChrizz for marco script!")
